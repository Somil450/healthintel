{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static",
      "config": {
        "distDir": "public"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "api/*.php": {
      "runtime": "php"
    }
  }
}
